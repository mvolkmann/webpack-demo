{
  "name": "webpack-demo",
  "version": "1.0.0",
  "description": "webpack demo",
  "main": "index.html",
  "scripts": {
    "build": "PRODUCTION=1 webpack",
    "clean": "rm bundle.js",
    "start": "./node_modules/.bin/webpack-dev-server --inline --content-base ./",
    "testWDS": "./node_modules/.bin/webpack-dev-server 'mocha!./test/*.js' --output-filename testBundle.js",
    "testW": "webpack test/*.js --output-filename test-bundle.js --target node; mocha test-bundle.js",
    "testM": "mocha!./test/*.js --output-filename test-bundle.js --target node; open index.html",
    "testK": "karma start --single-run --browsers PhantomJS",
    "test": "cd test; webpack; open index.html"
  },
  "author": "R. Mark Volkmann",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^5.8.22",
    "babel-loader": "^5.3.2",
    "chai": "^3.2.0",
    "css-loader": "^0.16.0",
    "eslint": "^1.1.0",
    "eslint-loader": "^1.0.0",
    "karma": "^0.13.9",
    "karma-chai": "^0.1.0",
    "karma-mocha": "^0.2.0",
    "karma-phantomjs-launcher": "^0.2.1",
    "karma-spec-reporter": "0.0.20",
    "karma-webpack": "^1.7.0",
    "less": "^2.5.1",
    "less-loader": "^2.2.0",
    "mocha": "^2.2.5",
    "mocha-loader": "^0.7.1",
    "phantomjs": "^1.9.18",
    "phantomjs-polyfill": "0.0.1",
    "style-loader": "^0.12.3",
    "webpack": "^1.11.0"
  }
}
